<?xml version="1.0"?>
<rdf:RDF xmlns="http://example.org/world#"
     xml:base="http://example.org/world"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    
    <owl:Ontology rdf:about="http://example.org/world">
        <rdfs:comment>A foundational ontology for modeling the world</rdfs:comment>
    </owl:Ontology>
    
    <!-- ========================================== -->
    <!-- TOP LEVEL: Entity (Everything that exists) -->
    <!-- ========================================== -->
    
    <owl:Class rdf:about="http://example.org/world#Entity">
        <rdfs:comment>The most general class - everything that exists</rdfs:comment>
    </owl:Class>
    
    <!-- ========================================== -->
    <!-- MAJOR DIVISIONS -->
    <!-- ========================================== -->
    
    <!-- Abstract vs Concrete -->
    <owl:Class rdf:about="http://example.org/world#AbstractEntity">
        <rdfs:subClassOf rdf:resource="http://example.org/world#Entity"/>
        <rdfs:comment>Things without physical existence: concepts, numbers, ideas</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/world#ConcreteEntity">
        <rdfs:subClassOf rdf:resource="http://example.org/world#Entity"/>
        <owl:disjointWith rdf:resource="http://example.org/world#AbstractEntity"/>
        <rdfs:comment>Things with physical or temporal existence</rdfs:comment>
    </owl:Class>
    
    <!-- ========================================== -->
    <!-- ABSTRACT ENTITIES -->
    <!-- ========================================== -->
    
    <owl:Class rdf:about="http://example.org/world#Concept">
        <rdfs:subClassOf rdf:resource="http://example.org/world#AbstractEntity"/>
        <rdfs:comment>Ideas, thoughts, mental constructs</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/world#Number">
        <rdfs:subClassOf rdf:resource="http://example.org/world#AbstractEntity"/>
        <rdfs:comment>Mathematical numbers</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/world#Property">
        <rdfs:subClassOf rdf:resource="http://example.org/world#AbstractEntity"/>
        <rdfs:comment>Qualities or characteristics (color, shape, etc.)</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/world#Relation">
        <rdfs:subClassOf rdf:resource="http://example.org/world#AbstractEntity"/>
        <rdfs:comment>Relationships between entities</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/world#Information">
        <rdfs:subClassOf rdf:resource="http://example.org/world#AbstractEntity"/>
        <rdfs:comment>Data, knowledge, messages</rdfs:comment>
    </owl:Class>
    
    <!-- ========================================== -->
    <!-- CONCRETE ENTITIES: Objects vs Processes -->
    <!-- ========================================== -->
    
    <owl:Class rdf:about="http://example.org/world#Object">
        <rdfs:subClassOf rdf:resource="http://example.org/world#ConcreteEntity"/>
        <rdfs:comment>Things that persist through time (continuants)</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/world#Process">
        <rdfs:subClassOf rdf:resource="http://example.org/world#ConcreteEntity"/>
        <owl:disjointWith rdf:resource="http://example.org/world#Object"/>
        <rdfs:comment>Events, activities, changes (occurrents)</rdfs:comment>
    </owl:Class>
    
    <!-- ========================================== -->
    <!-- OBJECTS: Physical vs Social -->
    <!-- ========================================== -->
    
    <owl:Class rdf:about="http://example.org/world#PhysicalObject">
        <rdfs:subClassOf rdf:resource="http://example.org/world#Object"/>
        <rdfs:comment>Material objects with mass and volume</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/world#SocialObject">
        <rdfs:subClassOf rdf:resource="http://example.org/world#Object"/>
        <rdfs:comment>Socially constructed entities: organizations, laws, money</rdfs:comment>
    </owl:Class>
    
    <!-- ========================================== -->
    <!-- PHYSICAL OBJECTS -->
    <!-- ========================================== -->
    
    <owl:Class rdf:about="http://example.org/world#LivingThing">
        <rdfs:subClassOf rdf:resource="http://example.org/world#PhysicalObject"/>
        <rdfs:comment>Organisms with biological life</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/world#NonLivingThing">
        <rdfs:subClassOf rdf:resource="http://example.org/world#PhysicalObject"/>
        <owl:disjointWith rdf:resource="http://example.org/world#LivingThing"/>
        <rdfs:comment>Inanimate physical objects</rdfs:comment>
    </owl:Class>
    
    <!-- Living Things -->
    <owl:Class rdf:about="http://example.org/world#Organism">
        <rdfs:subClassOf rdf:resource="http://example.org/world#LivingThing"/>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/world#Animal">
        <rdfs:subClassOf rdf:resource="http://example.org/world#Organism"/>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/world#Plant">
        <rdfs:subClassOf rdf:resource="http://example.org/world#Organism"/>
        <owl:disjointWith rdf:resource="http://example.org/world#Animal"/>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/world#Microorganism">
        <rdfs:subClassOf rdf:resource="http://example.org/world#Organism"/>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/world#Human">
        <rdfs:subClassOf rdf:resource="http://example.org/world#Animal"/>
        <rdfs:comment>Homo sapiens - humans</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/world#Mammal">
        <rdfs:subClassOf rdf:resource="http://example.org/world#Animal"/>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/world#Bird">
        <rdfs:subClassOf rdf:resource="http://example.org/world#Animal"/>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/world#Fish">
        <rdfs:subClassOf rdf:resource="http://example.org/world#Animal"/>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/world#Reptile">
        <rdfs:subClassOf rdf:resource="http://example.org/world#Animal"/>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/world#Insect">
        <rdfs:subClassOf rdf:resource="http://example.org/world#Animal"/>
    </owl:Class>
    
    <!-- Non-Living Things -->
    <owl:Class rdf:about="http://example.org/world#NaturalObject">
        <rdfs:subClassOf rdf:resource="http://example.org/world#NonLivingThing"/>
        <rdfs:comment>Objects formed by natural processes</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/world#Artifact">
        <rdfs:subClassOf rdf:resource="http://example.org/world#NonLivingThing"/>
        <owl:disjointWith rdf:resource="http://example.org/world#NaturalObject"/>
        <rdfs:comment>Human-made objects</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/world#CelestialBody">
        <rdfs:subClassOf rdf:resource="http://example.org/world#NaturalObject"/>
        <rdfs:comment>Astronomical objects: planets, stars, moons</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/world#GeologicalFormation">
        <rdfs:subClassOf rdf:resource="http://example.org/world#NaturalObject"/>
        <rdfs:comment>Mountains, rivers, rocks, minerals</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/world#Substance">
        <rdfs:subClassOf rdf:resource="http://example.org/world#PhysicalObject"/>
        <rdfs:comment>Matter: solid, liquid, gas</rdfs:comment>
    </owl:Class>
    
    <!-- Artifacts -->
    <owl:Class rdf:about="http://example.org/world#Tool">
        <rdfs:subClassOf rdf:resource="http://example.org/world#Artifact"/>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/world#Machine">
        <rdfs:subClassOf rdf:resource="http://example.org/world#Artifact"/>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/world#Vehicle">
        <rdfs:subClassOf rdf:resource="http://example.org/world#Artifact"/>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/world#Building">
        <rdfs:subClassOf rdf:resource="http://example.org/world#Artifact"/>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/world#Document">
        <rdfs:subClassOf rdf:resource="http://example.org/world#Artifact"/>
        <rdfs:comment>Written or recorded information carriers</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/world#Artwork">
        <rdfs:subClassOf rdf:resource="http://example.org/world#Artifact"/>
    </owl:Class>
    
    <!-- ========================================== -->
    <!-- SOCIAL OBJECTS -->
    <!-- ========================================== -->
    
    <owl:Class rdf:about="http://example.org/world#Organization">
        <rdfs:subClassOf rdf:resource="http://example.org/world#SocialObject"/>
        <rdfs:comment>Groups of people with common purpose</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/world#Institution">
        <rdfs:subClassOf rdf:resource="http://example.org/world#Organization"/>
        <rdfs:comment>Established organizations: governments, universities</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/world#Company">
        <rdfs:subClassOf rdf:resource="http://example.org/world#Organization"/>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/world#Community">
        <rdfs:subClassOf rdf:resource="http://example.org/world#SocialObject"/>
        <rdfs:comment>Social groups with shared identity</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/world#Role">
        <rdfs:subClassOf rdf:resource="http://example.org/world#SocialObject"/>
        <rdfs:comment>Social positions: teacher, parent, citizen</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/world#Law">
        <rdfs:subClassOf rdf:resource="http://example.org/world#SocialObject"/>
        <rdfs:comment>Legal rules and regulations</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/world#Currency">
        <rdfs:subClassOf rdf:resource="http://example.org/world#SocialObject"/>
        <rdfs:comment>Money and economic exchange systems</rdfs:comment>
    </owl:Class>
    
    <!-- ========================================== -->
    <!-- PROCESSES -->
    <!-- ========================================== -->
    
    <owl:Class rdf:about="http://example.org/world#NaturalProcess">
        <rdfs:subClassOf rdf:resource="http://example.org/world#Process"/>
        <rdfs:comment>Processes occurring in nature</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/world#BiologicalProcess">
        <rdfs:subClassOf rdf:resource="http://example.org/world#NaturalProcess"/>
        <rdfs:comment>Life processes: growth, metabolism, reproduction</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/world#PhysicalProcess">
        <rdfs:subClassOf rdf:resource="http://example.org/world#NaturalProcess"/>
        <rdfs:comment>Physical changes: motion, chemical reactions</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/world#Action">
        <rdfs:subClassOf rdf:resource="http://example.org/world#Process"/>
        <rdfs:comment>Intentional acts by agents</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/world#Communication">
        <rdfs:subClassOf rdf:resource="http://example.org/world#Action"/>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/world#Creation">
        <rdfs:subClassOf rdf:resource="http://example.org/world#Action"/>
        <rdfs:comment>Making or producing something</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/world#Movement">
        <rdfs:subClassOf rdf:resource="http://example.org/world#Process"/>
        <rdfs:comment>Change of location</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/world#SocialProcess">
        <rdfs:subClassOf rdf:resource="http://example.org/world#Process"/>
        <rdfs:comment>Interactions between social entities</rdfs:comment>
    </owl:Class>
    
    <!-- ========================================== -->
    <!-- SPACE AND TIME -->
    <!-- ========================================== -->
    
    <owl:Class rdf:about="http://example.org/world#SpatialRegion">
        <rdfs:subClassOf rdf:resource="http://example.org/world#AbstractEntity"/>
        <rdfs:comment>Regions of space: locations, areas, volumes</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/world#TemporalRegion">
        <rdfs:subClassOf rdf:resource="http://example.org/world#AbstractEntity"/>
        <rdfs:comment>Periods of time: moments, intervals, durations</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/world#TimeInstant">
        <rdfs:subClassOf rdf:resource="http://example.org/world#TemporalRegion"/>
        <rdfs:comment>A point in time</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/world#TimeInterval">
        <rdfs:subClassOf rdf:resource="http://example.org/world#TemporalRegion"/>
        <rdfs:comment>A duration between two time points</rdfs:comment>
    </owl:Class>
    
    <!-- ========================================== -->
    <!-- QUALITIES AND ATTRIBUTES -->
    <!-- ========================================== -->
    
    <owl:Class rdf:about="http://example.org/world#Quality">
        <rdfs:subClassOf rdf:resource="http://example.org/world#Property"/>
        <rdfs:comment>Characteristics that entities possess</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/world#PhysicalQuality">
        <rdfs:subClassOf rdf:resource="http://example.org/world#Quality"/>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/world#Color">
        <rdfs:subClassOf rdf:resource="http://example.org/world#PhysicalQuality"/>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/world#Shape">
        <rdfs:subClassOf rdf:resource="http://example.org/world#PhysicalQuality"/>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/world#Size">
        <rdfs:subClassOf rdf:resource="http://example.org/world#PhysicalQuality"/>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/world#Temperature">
        <rdfs:subClassOf rdf:resource="http://example.org/world#PhysicalQuality"/>
    </owl:Class>
    
    <!-- ========================================== -->
    <!-- FUNDAMENTAL OBJECT PROPERTIES -->
    <!-- ========================================== -->
    
    <!-- Participation in Processes -->
    <owl:ObjectProperty rdf:about="http://example.org/world#participatesIn">
        <rdfs:domain rdf:resource="http://example.org/world#Object"/>
        <rdfs:range rdf:resource="http://example.org/world#Process"/>
        <rdfs:comment>An object takes part in a process</rdfs:comment>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://example.org/world#hasParticipant">
        <owl:inverseOf rdf:resource="http://example.org/world#participatesIn"/>
        <rdfs:domain rdf:resource="http://example.org/world#Process"/>
        <rdfs:range rdf:resource="http://example.org/world#Object"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://example.org/world#performs">
        <rdfs:subPropertyOf rdf:resource="http://example.org/world#participatesIn"/>
        <rdfs:comment>Agent performs an action</rdfs:comment>
    </owl:ObjectProperty>
    
    <!-- Part-Whole Relationships -->
    <owl:ObjectProperty rdf:about="http://example.org/world#partOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://example.org/world#Entity"/>
        <rdfs:range rdf:resource="http://example.org/world#Entity"/>
        <rdfs:comment>Something is a part of something else</rdfs:comment>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://example.org/world#hasPart">
        <owl:inverseOf rdf:resource="http://example.org/world#partOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
    </owl:ObjectProperty>
    
    <!-- Spatial Relations -->
    <owl:ObjectProperty rdf:about="http://example.org/world#locatedIn">
        <rdfs:domain rdf:resource="http://example.org/world#Object"/>
        <rdfs:range rdf:resource="http://example.org/world#SpatialRegion"/>
        <rdfs:comment>Object is located in a spatial region</rdfs:comment>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://example.org/world#adjacentTo">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:domain rdf:resource="http://example.org/world#Object"/>
        <rdfs:range rdf:resource="http://example.org/world#Object"/>
        <rdfs:comment>Objects are next to each other</rdfs:comment>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://example.org/world#contains">
        <rdfs:domain rdf:resource="http://example.org/world#Object"/>
        <rdfs:range rdf:resource="http://example.org/world#Object"/>
    </owl:ObjectProperty>
    
    <!-- Temporal Relations -->
    <owl:ObjectProperty rdf:about="http://example.org/world#occursAt">
        <rdfs:domain rdf:resource="http://example.org/world#Process"/>
        <rdfs:range rdf:resource="http://example.org/world#TemporalRegion"/>
        <rdfs:comment>Process happens at a time</rdfs:comment>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://example.org/world#before">
        <rdfs:domain rdf:resource="http://example.org/world#TemporalRegion"/>
        <rdfs:range rdf:resource="http://example.org/world#TemporalRegion"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://example.org/world#after">
        <owl:inverseOf rdf:resource="http://example.org/world#before"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
    </owl:ObjectProperty>
    
    <!-- Causation -->
    <owl:ObjectProperty rdf:about="http://example.org/world#causes">
        <rdfs:domain rdf:resource="http://example.org/world#Process"/>
        <rdfs:range rdf:resource="http://example.org/world#Process"/>
        <rdfs:comment>One process causes another</rdfs:comment>
    </owl:ObjectProperty>
    
    <!-- Qualities -->
    <owl:ObjectProperty rdf:about="http://example.org/world#hasQuality">
        <rdfs:domain rdf:resource="http://example.org/world#Entity"/>
        <rdfs:range rdf:resource="http://example.org/world#Quality"/>
        <rdfs:comment>Entity possesses a quality</rdfs:comment>
    </owl:ObjectProperty>
    
    <!-- Social Relations -->
    <owl:ObjectProperty rdf:about="http://example.org/world#memberOf">
        <rdfs:domain rdf:resource="http://example.org/world#Human"/>
        <rdfs:range rdf:resource="http://example.org/world#Organization"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://example.org/world#hasRole">
        <rdfs:domain rdf:resource="http://example.org/world#Human"/>
        <rdfs:range rdf:resource="http://example.org/world#Role"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://example.org/world#relatedTo">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:domain rdf:resource="http://example.org/world#Human"/>
        <rdfs:range rdf:resource="http://example.org/world#Human"/>
        <rdfs:comment>General social relationship</rdfs:comment>
    </owl:ObjectProperty>
    
    <!-- Creation and Destruction -->
    <owl:ObjectProperty rdf:about="http://example.org/world#creates">
        <rdfs:domain rdf:resource="http://example.org/world#Creation"/>
        <rdfs:range rdf:resource="http://example.org/world#Entity"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://example.org/world#createdBy">
        <owl:inverseOf rdf:resource="http://example.org/world#creates"/>
    </owl:ObjectProperty>
    
    <!-- Information Relations -->
    <owl:ObjectProperty rdf:about="http://example.org/world#represents">
        <rdfs:domain rdf:resource="http://example.org/world#Information"/>
        <rdfs:range rdf:resource="http://example.org/world#Entity"/>
        <rdfs:comment>Information represents some entity</rdfs:comment>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://example.org/world#about">
        <rdfs:domain rdf:resource="http://example.org/world#Information"/>
        <rdfs:range rdf:resource="http://example.org/world#Entity"/>
        <rdfs:comment>Information is about some entity</rdfs:comment>
    </owl:ObjectProperty>
    
    <!-- ========================================== -->
    <!-- DATA PROPERTIES -->
    <!-- ========================================== -->
    
    <owl:DatatypeProperty rdf:about="http://example.org/world#hasName">
        <rdfs:domain rdf:resource="http://example.org/world#Entity"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="http://example.org/world#hasDescription">
        <rdfs:domain rdf:resource="http://example.org/world#Entity"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="http://example.org/world#hasMass">
        <rdfs:domain rdf:resource="http://example.org/world#PhysicalObject"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:comment>Mass in kilograms</rdfs:comment>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="http://example.org/world#hasVolume">
        <rdfs:domain rdf:resource="http://example.org/world#PhysicalObject"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:comment>Volume in cubic meters</rdfs:comment>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="http://example.org/world#hasAge">
        <rdfs:domain rdf:resource="http://example.org/world#Object"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:comment>Age in years</rdfs:comment>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="http://example.org/world#birthDate">
        <rdfs:domain rdf:resource="http://example.org/world#LivingThing"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="http://example.org/world#startTime">
        <rdfs:domain rdf:resource="http://example.org/world#Process"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="http://example.org/world#endTime">
        <rdfs:domain rdf:resource="http://example.org/world#Process"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="http://example.org/world#duration">
        <rdfs:domain rdf:resource="http://example.org/world#Process"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:comment>Duration in seconds</rdfs:comment>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="http://example.org/world#latitude">
        <rdfs:domain rdf:resource="http://example.org/world#SpatialRegion"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="http://example.org/world#longitude">
        <rdfs:domain rdf:resource="http://example.org/world#SpatialRegion"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
