@prefix : <http://worldmind.ai/core#>.
@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.

# Generic Classes
:Agent a rdfs:Class ;
    rdfs:label "Agent".

:TimeSpan a rdfs:Class ;
    rdfs:label "Time Span".

# Generic Properties
:hasTemporalExtent a rdf:Property ;
    rdfs:domain rdfs:Resource ;
    rdfs:range :TimeSpan.

:start a rdf:Property ;
    rdfs:domain :TimeSpan ;
    rdfs:range xsd:date.

:end a rdf:Property ;
    rdfs:domain :TimeSpan ;
    rdfs:range xsd:date.

:influencedBy a rdf:Property ;
    rdfs:label "influenced by" ;
    rdfs:comment "Indicates that one agent was influenced by another. Used as a proxy for 'studied under'." ;
    rdfs:domain :Agent ;
    rdfs:range :Agent.